Proceso Ejercicio27
	//Pedir un número y decir si es par o impar.
	
	//variables
	definir num como entero;
	
	//entradas
	Escribir 'Ingrese el numero aleatorio';
	leer num;
	
	//Salida
	si num mod 2=0 Entonces
		escribir num, ' es numero par ';
	SiNo
		escribir num, ' es numero impar ';
	FinSi
	
FinProceso
