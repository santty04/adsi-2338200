Proceso Ejercicio07
	// Hacer un algoritmo para sumar dos números, los cuales serán tecleados por el usuario. Mostrar el
	// resultado.
	Definir A Como Entero;
	Definir B Como Entero;
	Definir suma Como Entero;
	
	// Entrada
	Escribir " Digite un numero para sumar";
	Leer A;
	Escribir " Digite otro numero para sumar";
	Leer B;
	
	// operaciones
	suma = A + B;
	
	// Salida
	Escribir "El resultado de su suma es " , suma;
	
	
FinProceso
