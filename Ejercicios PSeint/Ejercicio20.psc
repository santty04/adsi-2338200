Proceso Ejercicio20
	//Pedir dos números y decir cuál es el mayor.
	
	//variables
	definir n1 Como Entero;
	definir n2 Como Entero;
	
	//entradas
	Escribir 'Escriba un numero';
	Leer n1;
	Escribir 'Escriba otro número';
	leer n2;
	
	//salidas
	si n1>n2 Entonces
