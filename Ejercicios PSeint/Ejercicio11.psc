Proceso Ejercicio11
	// Hacer un algoritmo que lea dos números enteros A y B y muestre su diferencia.
	
	// Definicion de variables
	Definir A Como Entero;
	Definir B Como Entero;
	Definir diferencia Como Entero;
	
	// Entradas 
	Escribir "Digite el numero A (siendo mayor que B)";
	Leer A;
	Escribir "Digite el numero B (siendo menor que A)";
	Leer B;
	
	// Operaciones
	diferencia = A - B;
	
	// Salida 
	Escribir "La diferencia que hay entre los numeros es " , diferencia;
	
FinProceso
