Proceso Ejercicio16
	//Calcular el área de un rectángulo de lados X e Y.
	
	//variables
	Definir A como real;
	Definir B Como Real;
	Definir perimetro como real;
	
	//entradas
	escribir "Ingrese la longitud del rectangulo";
	leer A;
	Escribir "Ingrese el ancho del rectangulo";
	leer B;
	
	//operaciones
	perimetro = A+B+A+B;
	
	//salida
	Escribir "El perimetro del rectangulo es :" , perimetro;
FinProceso
